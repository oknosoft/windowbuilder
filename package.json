{
  "name": "windowbuilder",
  "version": "2.0.236",
  "description": "Order dealer for windows and stained glass",
  "repository": {
    "type": "git",
    "url": "https://github.com/oknosoft/windowbuilder.git"
  },
  "engines": {
    "node": "^8.8",
    "npm": "^5.5"
  },
  "keywords": [
    "metadata",
    "order dealer",
    "cad",
    "crm",
    "window builder"
  ],
  "author": "Evgeniy Malyarov <info@oknosoft.ru> (http://www.oknosoft.ru)",
  "license": "SEE LICENSE IN LICENSE.en.md",
  "private": true,
  "bugs": {
    "url": "https://github.com/oknosoft/windowbuilder/issues"
  },
  "homepage": "http://light.oknosoft.ru/",
  "dependencies": {
    "@jscad/openjscad": "^1.6.1",
    "dxf-writer": "^1.0.8",
    "metadata-abstract-ui": "^2.0.16-beta.46",
    "metadata-core": "^2.0.16-beta.46",
    "metadata-dhtmlx": "^2.0.16-beta.46",
    "metadata-external": "^2.0.16-beta.46",
    "metadata-pouchdb": "^2.0.16-beta.46",
    "metadata-react": "^2.0.16-beta.46",
    "metadata-redux": "^2.0.16-beta.46",
    "paper-jsdom-canvas": "^0.11.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.6",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^2.3.0",
    "css-loader": "^0.28.8",
    "dotenv": "4.0.0",
    "eslint": "^4.15.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.6",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9",
    "gulp-base64": "^0.1",
    "gulp-concat": "^2.6",
    "gulp-csso": "^3.0.1",
    "gulp-rename": "^1.2",
    "gulp-replace": "^0.6.1",
    "gulp-strip-comments": "^2.5.2",
    "gulp-uglify": "^3.0",
    "gulp-umd": "^0.2",
    "gulp-wrap": "^0.13.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.6",
    "md5-file": "^3.2.3",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.10",
    "raf": "3.4.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6",
    "style-loader": "^0.19.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0",
    "webpack-manifest-plugin": "^1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint .",
    "meta:prebuild": "node scripts/meta.prebuild.js",
    "meta:link": "node scripts/meta.link.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
